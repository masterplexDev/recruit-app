            i.hideAutocomplete();
            i.emptyEmailBackdrop();
          });
        } else c.html(e), i.hideAutocomplete(), i.emptyEmailBackdrop();
      };
      i.isEmpty = function () {
        return n.trim(r.val()) === "" && r.val().length === 0 ? !0 : !1;
