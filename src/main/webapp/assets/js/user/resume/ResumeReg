      r.hidePopup = function () {
        f.hasClass("hidden") ||
          f.addClass("hidden").attr("aria-hidden", "true");
      };
      r.next = function () {
        e++;
        e > u.total && (e = 1);
        r.setPosPopup();
      };
      r.prev = function () {
        e--;
        e < 1 && (e = u.total);
        r.setPosPopup();
      };
      r.setPosPopup = function () {
        r.setHelp(e);
        y.text();
