        return (
          (r = _.uniq(i.words)),
          console.log(r),
          (u = new RegExp(r.join("|"), "gi")),
          (n = n.replace(/\n$/g, "\n\n").replace(u, "<mark>$&</mark>")),
          it && (n = n.replace(/ /g, " <wbr>")),
          n
        );
      };
