          n = u.pos[t][0] + g,
          s = u.pos[t][1] + nt,
          o = u.size[t][0],
          i = 0;
        n < 280 ? (i = o / 2 - 8) : ((i = n - 280 + (o / 2 - 8)), (n = 280));
        k.css("left", i + "px");
        f.css({ left: n + "px", top: s + "px" });
      };
      r.setHelp = function (n) {
        n = n - 1;
        var t = i.messages[n],
          r = i.words[n];
        f.find(".colHelp .description").html(t);
